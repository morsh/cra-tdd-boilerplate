{
  "name": "cra-tdd-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "start:no-browser": "BROWSER=none react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "yarn lint && yarn test:unit && yarn test:e2e",
    "test:unit": "react-app-rewired test --watchAll=false",
    "test:watch": "react-app-rewired test",
    "test:e2e": "start-server-and-test start:no-browser http://localhost:3000 e2e",
    "coverage": "yarn test:unit --coverage",
    "e2e": "cd e2e && jest",
    "lint": "eslint -c .eslintrc.js --ext .ts .",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "i18next": "^21.8.4",
    "i18next-http-backend": "^1.4.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.16.9",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-interactions": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/builder-webpack5": "^6.5.4",
    "@storybook/manager-webpack5": "^6.5.4",
    "@storybook/node-logger": "^6.5.4",
    "@storybook/preset-create-react-app": "^4.1.1",
    "@storybook/react": "^6.5.4",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.0.1",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/node": "^16.7.13",
    "@types/puppeteer": "^5.4.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.16.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest-puppeteer": "^6.1.0",
    "puppeteer": "^14.1.1",
    "react-app-rewire-multiple-entry": "^2.2.2",
    "react-app-rewired": "^2.2.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.4.2",
    "webpack": "^5.72.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "src/stories/",
      "src/__mocks__/",
      "src/index.ts",
      "src/second-entry.ts",
      "src/reportWebVitals.ts",
      "src/i18n/i18n.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
