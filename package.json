{
  "name": "cra-tdd-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:no-browser": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "yarn lint && yarn test:unit && yarn test:e2e",
    "test:unit": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:e2e": "start-server-and-test start:no-browser http://localhost:3000 test:e2e:cypress",
    "test:e2e:cypress": "cypress run",
    "lint": "tslint --project ./tsconfig.json",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@cypress/react": "^5.12.5",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cypress": "^9.6.1",
    "msft-tslint-config": "^1.0.0",
    "start-server-and-test": "^1.14.0",
    "tslint": "^6.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
